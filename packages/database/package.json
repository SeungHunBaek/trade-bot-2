{
  "name": "@passive-money/database",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "migrate": "typeorm migration:run -d dist/data-source.js",
    "migrate:generate": "typeorm migration:generate -d dist/data-source.js",
    "migrate:revert": "typeorm migration:revert -d dist/data-source.js"
  },
  "dependencies": {
    "@passive-money/config": "workspace:*",
    "@passive-money/common": "workspace:*",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
